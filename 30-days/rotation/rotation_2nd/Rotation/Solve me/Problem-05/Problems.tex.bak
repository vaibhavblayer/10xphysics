\node[thick,mirror,rotate=-180,scale=1.5,mirror decoration separation=0.08,mirror decoration amplitude=0.12] at (0,2) {};
\draw[line width=5,line cap=round] (0.08,3) coordinate (b)--([turn]-135:4) coordinate (a);
